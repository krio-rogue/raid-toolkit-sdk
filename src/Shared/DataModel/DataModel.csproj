        <ProjectReference Include="$(SourceDir)Shared\Common\Common.csproj" />
    </ItemGroup>

    <ItemGroup>
        <EmbeddedResource Include="Multipliers.json" />
    </ItemGroup>

    <PropertyGroup>
        <TargetFrameworks>net6.0-windows</TargetFrameworks>
        <LangVersion>10.0</LangVersion>
